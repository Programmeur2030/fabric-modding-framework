{
	"schemaVersion": 1,
	"id": "fabric-modding-framework",
	"version": "${version}",
	"name": "Fabric-modding-framework",
	"description": "met deze mod wordt het makelijker om je eigen mod te maken. Deze mod bevat delen van code van Kaupenjoe (MIT-licentie, Â© 2023 Kaupenjoe).",
	"authors": [
		"willemdeprogrammeur"
	],
	"contact": {
		"homepage": "https://www.willemdeprogrammeur.nl/",
		"sources": "https://github.com/Programmeur2030/fabric-modding-framework"
	},
	"license": "MIT",
	"icon": "assets/f-modding-framework/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"nl.willemdeprogrammeur.fabricmodframework.Fmoddingframework"
		],
		"fabric-datagen": [
			"nl.willemdeprogrammeur.fabricmodframework.FmoddingframeworkDataGenerator"
		]
	},
	"mixins": [
		"f-modding-framework.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}